{
  "mqttConnect": {
    "url": "mqtt://127.0.0.1",
    "username": "mqtt",
    "password": "password",
    "statePrefix": "Service Check"
  },

  "services": [
    {
      "name": "Local SSH",
      "interval": 600,
      "host": "127.0.0.1",
      "protocol": "tcp",
      "port": 22
    },
    {
      "name": "Intranet",
      "interval": 60,
      "host": "127.0.0.1",
      "protocol": "http"
    },
    {
      "name": "DNS Lookup",
      "interval": 60,
      "host": "localhost",
      "protocol": "dns"
    },
    {
      "name": "Public Web Site",
      "interval": 600,
      "host": "duckduckgo.com",
      "path": "/index.html",
      "protocol": "https"
    }
  ]
}
